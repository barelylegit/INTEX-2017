@model Intex_2017.Models.CallTicket

@{
    ViewBag.Title = "Delete";
}

<h2>Close Ticket</h2>

<h3>Are you sure you want to close this ticket?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Ticket Number:
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CallTicketID)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Subject):
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Subject)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Body):
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Body)
        </dd>
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Close" class="btn btn-danger" />
            <br />
            <br />
            @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-sm btn-default"})
        </div>
    }
</div>
